# Gemini API (Required for LLM normalization)
GEMINI_API_KEY="your_gemini_api_key_here"
GEMINI_MODEL=gemini-1.5-flash

# OpenRouter API (For LLM chatbot queries - Get key from https://openrouter.ai)
OPENROUTER_API_KEY="your_openrouter_api_key_here"
OPENROUTER_MODEL=nvidia/nemotron-3-nano-30b-a3b:free

# Optional: For OpenRouter rankings (leave empty if not needed)
SITE_URL=
SITE_NAME=Web Scraper LLM Query


# Browser Settings
HEADLESS_MODE=true
BROWSER_TIMEOUT=60000
VIEWPORT_WIDTH=1920
VIEWPORT_HEIGHT=1080

# OCR Settings
OCR_LANGUAGE=eng
OCR_DPI=300

# Output Settings
OUTPUT_FOLDER=output
LOG_LEVEL=INFO

# Request Settings
MAX_RETRIES=3
RETRY_DELAY=2

# Input File
URLS_FILE=urls.txt